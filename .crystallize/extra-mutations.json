                                }
                            ],
                            "components": [
                                {
                                    "componentId": "config",
                                    "contentChunk": {
                                        "chunks": [
                                            [
                                                {
                                                    "componentId": "hex",
                                                    "singleLine": {
                                                        "text": "#7ebab5"
                                                    }
                                                },
                                                {
                                                    "componentId": "3d-variant-attribute",
                                                    "singleLine": {
