    "create-paints": {
        "target": "pim",
        "mutation": "mutation CreateProduct($input: CreateProductInput!) { product { create(language: \"en\", input: $input, disableComponentValidation: true) { id } } }",
        "sets": [
            {
                "input": {
                    "tenantId": "$root.TENANT_ID",
