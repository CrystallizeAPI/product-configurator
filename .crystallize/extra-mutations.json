    "publish-everything": {
        "mutation": "mutation PublishItems($ids: [ID!]!) { publishItems(language: \"en\", ids: $ids, disableComponentValidation: true) { ... on BasicError { message errorName } ... on PublishItemsRequest { success { itemId } } } }",
