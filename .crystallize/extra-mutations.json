                            "componentId": "name",
                            "singleLine": {
                                "text": "Classic Colors"
                            }
                        },
                        {
                            "componentId": "description",
                            "richText": {
                                "json": [
                                    {
                                        "kind": "block",
                                        "type": "paragraph",
                                        "textContent": "A selection of classic frame colors."
                                    }
                                ]
                            }
                        }
                    ],
                    "vatTypeId": "$root.TENANT_DEFAULT_VATTYPE_ID",
                    "variants": [
                        {
                            "sku": "copper-patina",
                            "name": "Copper Patina",
                            "isDefault": true,
                            "priceVariants": [
                                {
                                    "identifier": "default",
                                    "price": 100
                                }
                            ],
                            "stockLocations": [
                                {
                                    "identifier": "default",
                                    "stock": 100
                                }
                            ],
                            "attributes": [
                                {
                                    "attribute": "Color",
                                    "value": "Copper Patina"
                                }
                            ],
                            "images": [
